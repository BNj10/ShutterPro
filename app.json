{
  "expo": {
    "name": "ShutterPro",
    "slug": "ShutterPro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/main-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/main-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.benj10.ShutterPro"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/main-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9fdab926-2ad3-4dad-b62e-5e3d1bfe0740"
      }
    }
  }
}
